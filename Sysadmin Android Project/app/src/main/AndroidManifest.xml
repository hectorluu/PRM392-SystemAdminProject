<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.systemadminproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SystemAdminProject"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.storeApprove.ViewStoreApproveDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.storeApprove.ViewStoreApproveActivity"
            android:exported="false" />
        <activity
            android:name=".activity.store.ViewStoreDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.store.ViewStoreActivity"
            android:exported="false" />
        <activity
            android:name=".activity.user.ViewUserActivity"
            android:exported="false" />
        <activity
            android:name=".activity.brand.ViewBrandDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.brand.ViewBrandActivity"
            android:exported="false" />
        <activity
            android:name=".activity.main.MainActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".activity.login.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>